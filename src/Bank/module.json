{
	"name": "Bank",
	"version": "2.1.8",
	"title": "Bank framework",
	"model": [
		"Bank_Receipt",
		"Bank_Backend"
	],
	"init": {
		"Monitor_Tag": [
			{
				"name": "receipt",
				"description": "Tag for monitors on receipt metrics"
			}
		],
		"Monitor_Metric": [
			{
				"name": "paid_amount",
				"title": "Total paid ammount",
				"description": "Total amount of paid receipts.",
				"function": "Bank_Monitor::paidAmount",
				"cacheable": false
			}
		]
	},
	"init_assoc": {
		"Monitor_Tag|Monitor_Metric": [
			{
				"from": "name='receipt'",
				"to": "name='paid_amount'"
			}
		]
	}
}